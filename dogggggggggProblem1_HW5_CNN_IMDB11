        "# We iterate all the corpus through our cleaning functions and tokenize it\n",
        "# You can use some of the previously defined functions\n",
        "#### YOUR CODE HERE ####\n",
        "train_data = [tokenize(basic_clean(review)) for review in train_texts]\n",
        "test_data = [tokenize(basic_clean(review)) for review in test_texts]"
      ]
