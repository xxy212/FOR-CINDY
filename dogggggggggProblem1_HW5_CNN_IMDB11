      "source": [
        "# find the longest document\n",
        "test_corpus = open(TEST_FEAT, encoding=\"utf-8\").read().strip().splitlines()\n",
        "train_corpus = open(TRAIN_FEAT, encoding=\"utf-8\").read().strip().splitlines()\n",
        "max__test_length = max(len(doc.split()) for doc in test_corpus)\n",
        "max_train_length = max(len(doc.split()) for doc in train_corpus)\n",
        "print(max(max_train_length, max__test_length))\n"
      ]
